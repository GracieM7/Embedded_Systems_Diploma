#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int n,e,l;
	printf("Enter no of elements:");
	fflush(stdout);
	scanf("%d",&n);

	int arr[n];

	printf("Enter elements of the array:\n");
	fflush(stdout);
	for(int i=0;i<n;i++){
		scanf("%d",&arr[i]);
	}

	printf("Enter the element to be inserted:");
	fflush(stdout);
	scanf("%d",&e);
	printf("Enter the location:");
	fflush(stdout);
	scanf("%d",&l);

	arr[l-1]=e;

	for(int i=0;i<n;i++){
			printf("%d ",arr[i]);
		}

    return 0;
}
