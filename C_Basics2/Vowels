#include <stdio.h>
#include <stdlib.h>

int main(void) {
	char a;
	printf("Enter a character:");
	scanf("%c", &a);

	switch (a) {
	case 'a':
	case 'e':
	case 'u':
	case 'i':
	case 'o':
	case 'A':
	case 'U':
	case 'I':
	case 'O':
	case 'E':
		printf("%c is a vowel", a);
		break;
	default:

		printf("%c is a consonant", a);
	}

	return EXIT_SUCCESS;
}
