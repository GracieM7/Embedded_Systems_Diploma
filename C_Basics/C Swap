#include <stdio.h>
#include <stdlib.h>

int main(void) {
	float a,b,c;
	printf("Enter value of a:\n");
	scanf("%f",&a);
	printf("Enter value of b:\n");
	scanf("%f",&b);
	c=a;
	a=b;
	b=c;
	printf("After swapping, value of a = %f\n",a);
	printf("After swapping, value of b = %f\n",b);
	return EXIT_SUCCESS;
}
